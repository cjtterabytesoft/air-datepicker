{
    "name": "cjtterabytesoft/air-datepicker",
    "description": "Air DatePicker. [Yii 3.0/Jquery]",
    "keywords": ["yii3", "jquery", "widget", "air-datepicker"],
    "homepage": "https://github.com/cjtterabytesoft/air-datepicker",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/cjtterabytesoft/air-datepicker/issues",
        "source": "https://github.com/cjtterabytesoft/air-datepicker/widgets",
        "forum":  "https://discuss.cjtterabyte.com"
    },
    "authors": [
        {
            "name": "Wilmer Arambula",
            "email": "github@cjtterabyte.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2": "~3.0@dev",
        "bower-asset/air-datepicker": "^2.2"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "2.1.x-dev",
        "yiisoft/yii2-gii": "2.1.x-dev",
        "phpunit/phpunit": "~7.0.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {"cjtterabytesoft\\widgets\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"cjtterabytesoft\\widgets\\tests\\": "tests"}
    },
    "config": {
        "process-timeout": 1800
    }
}
